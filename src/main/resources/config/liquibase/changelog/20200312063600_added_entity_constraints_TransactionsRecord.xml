<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
    <!--
        Added the constraints for entity TransactionsRecord.
    -->
    <changeSet id="20200312063600-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="customer_id"
                                 baseTableName="transactions_record"
                                 constraintName="fk_transactions_record_customer_id"
                                 referencedColumnNames="id"
                                 referencedTableName="customer"/>

        <addForeignKeyConstraint baseColumnNames="account_id"
                                 baseTableName="transactions_record"
                                 constraintName="fk_transactions_record_account_id"
                                 referencedColumnNames="id"
                                 referencedTableName="accounts"/>

        <addForeignKeyConstraint baseColumnNames="invoice_id"
                                 baseTableName="transactions_record"
                                 constraintName="fk_transactions_record_invoice_id"
                                 referencedColumnNames="id"
                                 referencedTableName="invoice"/>

    </changeSet>
</databaseChangeLog>
